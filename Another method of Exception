#include<iostream>
using namespace std;
int main()
{
    cout<<"Enter two integers:";
    int num1,num2;
    cin>>num1>>num2;
    try{
        if(num2==0)
        throw num2;
        cout<<num1<<"/"<<num2<<"is:"<<(num1/num2)<<endl;
    }
    catch(int e)
    {
        cout<<"Exceotion : there is division by number"<<e<<endl;
    }
    
    cout<<"Execution continues.."<<endl;
    return 0;
}
