#include <iostream>
using namespace std;
template <typename T1, typename T2, typename T3> class Temp
{
  public:
    T1 x;
    T2 y;
    T3 z;
    Temp(T1 val1, T2 val2, T3 val3) : x(val1), y(val2), z(val3)
    {
    }
    void getValues()
    {
        cout << x << " " << y << " " << z;
    }
};

int main()
{
    Temp<int, double, string> intDoubleStringTemp(10, 3.14, "Hello");
    Temp<char, float, bool> charFloatBoolTemp('A', 5.67f, true);
intDoubleStringTemp.getValues();
    cout << endl;
    charFloatBoolTemp.getValues();

    return 0;
}
