#include <iostream>
using namespace std;
class A
{
    public:
    void show()
    {
        cout<<"This is class A"<<endl;
    }
};
class B:public A
{
    public:
    void show1()
    {
        cout<<"This is class B"<<endl;
    }
};
class C: public A
{
    public:
    void display()
    {
        cout<<"This is class C"<<endl;
    }
};

int main()
{
   A ob;
   B ob1;
   return 0;
}
