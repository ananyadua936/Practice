#include <iostream>
using namespace std;
class A
{
    int x;
    public:
    void input(int y)
    {
        x = y;
    }
    A operator +(A ob4)
    {
        A a;
        a.x= x+ob4.x;
        return a;
    }
     void show()
     {
         cout<<x;
     }
};
int main()
{
    A ob1,ob2,ob3;
    ob1.input(6);
    ob2.input(7);
    ob3=ob1+ob2;
    ob3.show();
    return 0;
}
