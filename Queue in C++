#include <iostream>
#include <stack>
using namespace std;

int main() {
    int arr[] = {1, 2, 3, 4, 5};
    int n = 5;
    
    stack<int> s;
    
    // Push all elements to stack
    for(int i = 0; i < n; i++) {
        s.push(arr[i]);
    }
    
    // Pop from stack (automatically reverses)
    cout << "Reverse order: ";
    while(!s.empty()) {
        cout << s.top() << " ";
        s.pop();
    }
    
    return 0;
}![Uploading image.pngâ€¦]()
