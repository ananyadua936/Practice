#include <iostream>
#include <queue>
using namespace std;

int main() {
    queue<string> serviceQueue;
    serviceQueue.push("Customer_1");
    serviceQueue.push("Customer_2");
    serviceQueue.push("Customer_3");

    cout << "Serving customers:\n";
    while (!serviceQueue.empty()) {
        cout << serviceQueue.front() << " is being served.\n";
        serviceQueue.pop(); // Remove the served customer
    }

    return 0;
}
