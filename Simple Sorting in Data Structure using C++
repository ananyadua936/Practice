#include <algorithm>
#include <iostream>
#include <vector>
using namespace std;

int main
(
    int nums[] = {5, 3, 8, 1};
    int n = sizeof(nums) / sizeof(nums[0]);
    sort(nums, nums + n);
    cout << "Sorted array: ";
    for (int i = 0; i < n; i++)
    cout << nums[i] << " ";
    cout << endl;
    vector<int> list = {5, 3, 8, 1};
    sort(list.begin(), list.end());
    cout << "Sorted vector: ";
    for (int num : list)
    cout << num << " ";
     return 0;
}
