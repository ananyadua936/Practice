#include <iostream>
#include <string>
using namespace std;
int main()
{
    string str1,str2;
     string combined;
    cout<<"Enter first word";
    cin>>str1;
    cout<<"Enter second word";
    cin>>str2;
    combined= str1+str2;
    cout<<str1+str2;
    cout<<"concatenated string:"<<combined;
    return 0;
    
}
