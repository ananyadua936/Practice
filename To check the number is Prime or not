#include <iostream>
using namespace std;

int main() {
    int n;
    cout<<"Enter any number";
    cin>> n;
    int count = 0;
    if (n <= 1)
        cout << n << " is NOT prime";
    else {
for (int i = 1; i <= n; i++) {
            if (n % i == 0)
                count++;
        }
  if (count > 2)
            cout << n << " is NOT prime";
            else
            cout << n << " is prime";
    }
    return 0;
}
