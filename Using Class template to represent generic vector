#include <iostream>
#include <vector>
using namespace std;
template <class T>
class MyVector {
private:
    T arr[100]; 
    int size;

public:
    MyVector() {
        size = 0;
    }
    void add(T element) {
        arr[size] = element;
        size++;
    }
    void show() {
        cout << "(";
        for(int i = 0; i < size; i++) {
            cout << arr[i];
            if(i < size - 1) cout << ",";
        }
        cout << ")" << endl;
    }
};
int main() {
    MyVector<int> v1;
    v1.add(10);
    v1.show();  
    MyVector<double> v2;
    v2.add(3.14);
    v2.show();
    return 0;
}
